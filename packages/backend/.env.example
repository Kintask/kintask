# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Web3.storage API Token
WEB3STORAGE_TOKEN=your_web3.storage_api_token

# Recall Network (replace with actual details if provided)
RECALL_API_KEY=your_recall_api_key # Placeholder
RECALL_API_ENDPOINT=https://testnet.recall.network/api # Placeholder

# Backend Server Port
PORT=3001

# Filecoin Knowledge Graph Index CID (from scripts/upload-kg-fragments.js output)
KB_INDEX_CID=PASTE_INDEX_CID_HERE

# L2 Network RPC URL (must match contracts/.env)
L2_RPC_URL=https://sepolia.base.org

# Backend Wallet Private Key (must match contracts/.env for sending Tx)
# IMPORTANT: Use a dedicated burner wallet for hackathons/testing!
WALLET_PRIVATE_KEY=0xYourPrivateKeyHere

# Deployed KintaskCommitment Contract Address (from contracts deployment output)
KINTASK_CONTRACT_ADDRESS=PASTE_DEPLOYED_ADDRESS_HERE

# Blocklock Sender Proxy Address (must match contracts/.env)
BLOCKLOCK_SENDER_PROXY_ADDRESS=0xBlocklockProxyAddressOnYourL2
