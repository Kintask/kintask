# packages/contracts/.env.example

# L2 Testnet RPC URL (e.g., Base Sepolia from Alchemy/Infura)
L2_RPC_URL="https://sepolia.base.org"

# Burner Wallet Private Key for Deployment
# IMPORTANT: Generate a new wallet for testing! Do not use main funds.
WALLET_PRIVATE_KEY="0xyour_burner_wallet_private_key"

# Etherscan/Blockscout API Key for Contract Verification (Optional)
ETHERSCAN_API_KEY="YourEtherscanApiKey"

# Blocklock Sender Proxy Address for the chosen L2 Testnet (from Blocklock Docs)
BLOCKLOCK_SENDER_PROXY_ADDRESS="0xBlocklockProxyAddressOnYourL2"